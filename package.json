{
	"name": "nathanblaylock.com",
	"version": "2.5.1",
	"private": true,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./icons": {
			"types": "./dist/icons.d.ts"
		},
		"./styles": {
			"default": "./dist/nathanblaylock.min.css"
		}
	},
	"scripts": {
		"blunt": "node ./node_modules/@ngblaylock/blunt-images ./blunt.config.cjs",
		"dev": "vite dev --open",
		"build": "npm run blunt & vite build",
		"postbuild": "npx @sveltejs/package --input src/PACKAGE/components && sass --no-source-map --style=compressed src/sass/style.scss:dist/nathanblaylock.min.css",
		"preview": "vite preview --open",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@ngblaylock/blunt-images": "1.0.0-beta.5",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/package": "^2.4.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/aos": "^3.0.7",
		"@types/bootstrap": "^5.2.10",
		"@types/chroma-js": "^3.1.1",
		"@types/lodash": "^4.17.19",
		"@types/node": "^24.0.7",
		"@types/sortablejs": "^1.15.8",
		"aos": "^2.3.4",
		"chroma-js": "^3.1.2",
		"mdsvex": "^0.12.6",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"rehype-external-links": "^3.0.0",
		"sass": "1.77.6",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"typescript": "^5.0.0",
		"unplugin-svelte-components": "^0.3.1",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@mdi/js": "^7.4.47",
		"@ngblaylock/bootstrap-extensions": "0.1.1",
		"@sveltejs/kit": "^2.25.2",
		"@tiptap/core": "^3.0.7",
		"@tiptap/extension-highlight": "^3.0.7",
		"@tiptap/extensions": "^3.0.7",
		"@tiptap/pm": "^3.0.7",
		"@tiptap/starter-kit": "^3.0.7",
		"bootstrap": "5.3.7",
		"lodash": "^4.17.21",
		"shiki": "^3.7.0",
		"sortablejs": "^1.15.6",
		"svelte": "^5.36.14",
		"svelte-portal": "^2.2.1"
	}
}

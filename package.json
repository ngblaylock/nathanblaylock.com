{
	"name": "nathanblaylock.com",
	"version": "2.3.0",
	"private": true,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./icons": {
			"types": "./dist/icons.d.ts"
		},
		"./styles": {
			"default": "./dist/nathanblaylock.min.css"
		}
	},
	"scripts": {
		"blunt": "node ./node_modules/@ngblaylock/blunt-images ./blunt.config.cjs",
		"dev": "vite dev --open",
		"build": "npm run blunt & vite build",
		"postbuild": "npx @sveltejs/package --input src/lib/PACKAGE && sass --no-source-map --style=compressed src/sass/style.scss:dist/nathanblaylock.min.css",
		"preview": "vite preview --open",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@ngblaylock/blunt-images": "^1.0.0-beta.5",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/package": "^2.3.9",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/aos": "^3.0.7",
		"@types/chroma-js": "^2.4.0",
		"@types/lodash": "^4.17.15",
		"@types/node": "^22.10.10",
		"@types/sortablejs": "^1.15.8",
		"aos": "^2.3.4",
		"chroma-js": "^2.4.2",
		"mdsvex": "^0.12.3",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.2.6",
		"rehype-external-links": "^3.0.0",
		"sass": "^1.64.2",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"typescript": "^5.5.0",
		"unplugin-svelte-components": "^0.3.1",
		"vite": "^5.4.14"
	},
	"dependencies": {
		"@mdi/js": "^7.4.47",
		"@ngblaylock/bootstrap-extensions": "0.1.0-beta.20",
		"@sveltejs/kit": "^2.8.3",
		"bootstrap": "^5.3.3",
		"lodash": "^4.17.21",
		"shiki": "^2.1.0",
		"sortablejs": "^1.15.6",
		"svelte": "^5.0.0",
		"svelte-portal": "^2.2.1"
	}
}
